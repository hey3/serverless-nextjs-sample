{
  "name": "serverless-nextjs-sample",
  "description": "Example for serverless Next.js components.",
  "version": "0.1.0",
  "private": true,
  "author": "hey3",
  "keywords": [
    "nextjs",
    "serverless",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hey3/serverless-nextjs-sample"
  },
  "bugs": {
    "url": "https://github.com/hey3/serverless-nextjs-sample/issues"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy:staging": "serverless --stage=staging",
    "deploy:prod": "serverless --stage=prod",
    "remove:staging": "serverless remove --stage=staging",
    "remove:prod": "serverless remove --stage=prod",
    "format": "prettier --check './src/**/*.{ts,tsx,json}'",
    "format:fix": "prettier --write './src/**/*.{ts,tsx,json}'",
    "lint": "eslint './src/**/*.{ts,tsx,json}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx,json}'",
    "typecheck": "tsc --project tsconfig.json --pretty --noEmit"
  },
  "dependencies": {
    "next": "10.0.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "swr": "^0.3.9"
  },
  "devDependencies": {
    "@sls-next/serverless-component": "^1.18.0",
    "@serverless/core": "^1.1.2",
    "@serverless/template": "^1.1.4",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.1.2",
    "serverless": "^2.15.0",
    "typescript": "^4.1.2"
  }
}
